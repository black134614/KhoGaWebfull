@using KhoGaWebfull.Models;
@{
    ViewBag.Title = "Trang chủ";
    ViewBag.Robots = "index,follow";
    ViewBag.Description = "mo ta seo";

}

<main class="main__content_wrapper">
    <!-- Start slider section -->
    @{ Html.RenderAction("HomeBanner", "Home");}
    <!-- End slider section -->
    <!-- Start collection section -->
    <section class="shop__collection--section section--padding">
        <div class="container">
            <div class="section__heading text-center mb-40">
                <h2 class="section__heading--maintitle">SẢN PHẨM BÁN CHẠY</h2>
            </div>
            <div class="shop__collection--column5 swiper">
                <div class="swiper-wrapper">
                    @{
                        if (ViewBag.ChoiceProducts != null)
                        {
                            foreach (Product item in ViewBag.ChoiceProducts)
                            {
                                string url = item.Title + "-" + item.ProductID;
                                double productPrice = (double)item.UnitPrice + (double)item.Discount;
                                <div class="swiper-slide">
                                    <div class="shop__collection--card text-center">
                                        <a class="shop__collection--link" href="@url">
                                            <img class="shop__collection--img" src="@item.PicturePath" alt="@item.Title">
                                            <h3 class="shop__collection--title">@item.Title</h3>
                                            <div class="product__card--price">
                                                <span class="current__price money">@item.UnitPrice</span>
                                                <span class="old__price money">@productPrice</span>
                                            </div>
                                            <div class="shop__collection--subtitle">
                                                <span>Số lượng:</span>
                                                <span class="quantity">@item.UnitInStock</span>
                                            </div>
                                        </a>
                                    </div>
                                </div>
                            }
                        }
                        else
                        {
                            <div>Không có sản phẩm!</div>
                        }
                    }
                </div>
                <div class="swiper__nav--btn swiper-button-next">
                    <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class=" -chevron-right"><polyline points="9 18 15 12 9 6"></polyline></svg>
                </div>
                <div class="swiper__nav--btn swiper-button-prev">
                    <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class=" -chevron-left"><polyline points="15 18 9 12 15 6"></polyline></svg>
                </div>
            </div>
        </div>
    </section>
    <!-- End collection section -->
    <!-- Start image width text section -->
    <section class="image__width--text__section position-relative">
        <div class="container">
            <div class="image__width--text__inner d-flex position-relative">
